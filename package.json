{
  "name": "ng-alain-vscode",
  "displayName": "NG-ALAIN Snippets",
  "description": "Snippets based on NG-ALAIN",
  "version": "11.0.0",
  "icon": "ng-alain.png",
  "publisher": "cipchk",
  "license": "MIT",
  "licenseUrl": "LICENSE",
  "homepage": "https://github.com/cipchk/ng-alain-vscode/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/cipchk/ng-alain-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/cipchk/ng-alain-vscode/issues",
    "email": "cipchk@qq.com"
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "watch": "gulp watch",
    "build": "gulp build",
    "resource": "node scripts/snippet.js",
    "release": "npm run resource && gulp package"
  },
  "engines": {
    "vscode": "^1.30.x"
  },
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "main": "./out/extension",
  "categories": [
    "Snippets",
    "Other",
    "Programming Languages"
  ],
  "extensionPack": [
    "cipchk.ng-zorro-vscode"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:html"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ng-alain-vscode.cache",
        "title": "%ng-alain-vscode.cache%"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets-ts.json"
      },
      {
        "language": "html",
        "path": "./snippets-html.json"
      }
    ]
  },
  "dependencies": {
    "vscode-nls": "^5.0.0",
    "less": "^3.11.2"
  },
  "devDependencies": {
    "vscode-snippet-generator": "^1.3.2",
    "vscode": "^1.1.26",
    "vsce": "^1.81.1",
    "gulp": "^4.0.2",
    "gulp-filter": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "del": "^4.1.1",
    "event-stream": "^4.0.1",
    "vscode-nls-dev": "^3.3.2"
  }
}
